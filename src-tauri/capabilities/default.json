{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": ["*"],
  "remote": {
    "urls": ["http://localhost:*"]
  },
  "permissions": [
    "core:default",
    "updater:default",
    "updater:allow-check",
    "updater:allow-download",
    "updater:allow-install",
    "process:allow-restart"
  ]
}
